<tr id=<%= @var['id']%>:container >
<th><%= label_tag(@var['id'], @var['label']) %></th>
<td>
<% multiname = @var['id'] + 'Multi'%>
<table id=<%= multiname %>><tbody>
<tr>
    <th></th>
    <th></th>
    <th>
        <%= image_tag(image_url("redo-icon.png"), alt: "undo", :style=>"display: none", :onclick=>"twd_grow_undo(this); return false;", :id => multiname + ':undo') %>
    </th>
</tr>
<% multiname0 = multiname + ':0:' %> 
<tr id=<%= multiname %>:0 style="display:none">
    <td>
        <div id=<%= multiname0 %><%=@var['id']%> name=""   type="text" onchange="twd_grow_add(this);" ></div>
        <div id=<%= multiname0 %><%=@var['id']%>:bs_container>
            <%#= radio_button_tag(@var['id'], 'text', true, :name => @var['id'] + 'Multi:0:'+ @var['id'] +'_bs_group') %>
            <%= radio_button_tag(@var['id'], 'text', true, :name => multiname0 + @var['id'] +'_bs_group') %>
            <%= label_tag(@var['id'], 'Url') %>
            <%= radio_button_tag( @var['id'], 'file', false, :name => multiname0 + @var['id'] +'_bs_group') %>
            <%= label_tag(@var['id'], 'File Upload') %>
            <% if @bs_private %>
                <%= radio_button_tag( @var['id'], 'select', false, :name =>  multiname0 + @var['id'] +'_bs_group') %>
                <%= label_tag(@var['id'], 'Select') %>
            <% end %>
            <%= text_field_tag multiname0 +'text', '' %>
            <%= file_field_tag multiname0 +'file', :style => 'display: none; ' %>
            <% if @bs_private %>
                <%= select_tag( multiname0 + 'sselect', options_for_select(@files_from_hts), :style => 'display: none;') %>
            <% end %>

    </td>
    <td>
        <%= image_tag(image_url("undo-icon.png"), alt: "delete file", :style=>"display: none", :onclick=>"twd_grow_del(this); return false;",:name => multiname0 +'del', :id => multiname0 + 'del') %>
    </td>
</tr>

<% multiname1 = multiname + ':1:' %> 
<tr id=<%= multiname %>:1>
    <td> 
        <div id=<%= multiname1 %><%=@var['id']%> name=""   type="text" onchange="twd_grow_add(this);" ></div>
        <div id=<%= multiname1 %><%=@var['id']%>:bs_container>
        <%= radio_button_tag(@var['id'], 'text', true, :name => multiname1 + @var['id'] +'_bs_group') %>
        <%= label_tag(@var['id'], 'Url') %>
        <%= radio_button_tag(@var['id'], 'file', false, :name => multiname1 + @var['id'] +'_bs_group') %>
        <%= label_tag(@var['id'], 'File Upload') %>
        <% if @bs_private %>
                <%= radio_button_tag( @var['id'], 'select', false, :name => multiname1 + @var['id'] + '_bs_group') %>
                <%= label_tag(@var['id'], 'Select') %>
        <% end %>
        <%= text_field_tag multiname1 + 'text', '',  required: @var['required'] %>
        <%= file_field_tag multiname1 + 'file',  :style => 'display: none; ' %>

        <% if @bs_private %>
                <%= select_tag( multiname1 + 'select', options_for_select(@files_from_hts), :style => 'display: none;') %>
        <% end %>
        </div>
    </td>
    <td> 
        <%= image_tag(image_url("undo-icon.png"), alt: "delete file", :style=>"display: none", :onclick=>"twd_grow_del(this); return false;",:name=> multiname1 + 'del', :id=> multiname1 + 'del') %>
    </td>
</tr>
</tbody></table>
</td>
</tr>
